import java.io.BufferedReader
import java.io.InputStreamReader

val br = BufferedReader(InputStreamReader(System.`in`))
val bw = System.out.bufferedWriter()

fun main() {
	for(i in br.readLine().toInt() downTo 1){
		for(j in 0 until i)
			bw.write("*")
		bw.write("\n")
	}

	bw.close()
	br.close()
}
